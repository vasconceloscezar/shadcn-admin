{
  "name": "enterprise-template",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm dev:ui\" \"pnpm dev:agents\"",
    "dev:ui": "pnpm --filter ui dev",
    "dev:agents": "echo 'Python agent not set up yet'",
    "build": "pnpm --filter './apps/*' --filter './packages/*' build",
    "build:ui": "pnpm --filter ui build",
    "lint": "pnpm --filter './apps/*' --filter './packages/*' lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,json,md}\"",
    "typecheck": "pnpm --filter ui typecheck",
    "clean": "pnpm --filter './apps/*' --filter './packages/*' clean && rm -rf node_modules",
    "setup": "pnpm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "prettier": "^3.6.2"
  }
}
